{
  "manifest_version": 2,
  "name": "realkittenhours",
  "version": "0.1",
  "web_accessible_resources": ["nut.png"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup/public/index.html",
    "default_title": "a popup will come here"
  }
}
